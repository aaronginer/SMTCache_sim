# physical-cores: number of phsyical cores of the system
# logical-cores: total number of logical cores across all physical cores
# caches-per-core: number of l1 caches for each core, KERNEL cache included
# parse-time: parsing will stop after [parse-time] seconds have passed
# pipe: use stdin as program input 

[DEFAULT]
physical-cores = 4
logical-cores = 4
caches-per-core = 5
parse-time = 10000
trace-file = trace_4_cores.x
pipe = no

[cfg_4_4]
physical-cores = 4

[cfg_4_8]
logical-cores = 8

[cfg_1_core_1_2]
trace-file = trace_snapshot_1_cores.x
physical-cores = 1
logical-cores = 2

[cfg_4_core_stress]
trace-file = trace_snapshot_stress_4_cores.x

[pipe_4_core_stress]
pipe = yes
